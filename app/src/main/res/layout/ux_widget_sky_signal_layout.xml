<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/color_444444"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">


<!--飞机信号-->
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/iv_st"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hd_white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <com.gdu.demo.widgetlist.signal.SignalQuality
        android:id="@+id/tv_st_quality"
        android:layout_width="26dp"
        android:layout_height="24dp"
        android:layout_marginStart="2dp"
        app:layout_constraintLeft_toRightOf="@+id/iv_st"
        app:layout_constraintTop_toTopOf="@id/iv_st" />

</merge>