<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:visibility="visible">

        <TextView
            android:id="@+id/tv_battery_title"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40"
            android:gravity="center_vertical"
            android:text="@string/string_battery_info"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_12"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_drtk_battery"
            android:layout_width="@dimen/dp_52"
            android:layout_height="@dimen/dp_136"
            android:src="@drawable/drtk_battery"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_battery_title"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginLeft="@dimen/dp_10"/>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_in_battery"
            android:layout_width="0dp"
            android:layout_height="@dimen/dp_45"
            app:layout_constraintTop_toTopOf="@id/iv_drtk_battery"
            app:layout_constraintLeft_toRightOf="@id/iv_drtk_battery"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="@dimen/dp_30"
            android:background="@drawable/stroke_8e8e94_radius_2_bg">


            <TextView
                android:id="@+id/tv_in_battery_vol_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:text="@string/Label_Voltage"
                android:textSize="@dimen/sp_12"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                app:layout_constraintTop_toBottomOf="@id/tv_in_battery_vol"
                android:textColor="@color/color_2D2D2D" />


            <TextView
                android:id="@+id/tv_in_battery_vol"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_3"
                android:text="--"
                android:textSize="@dimen/sp_12"
                app:layout_constraintLeft_toLeftOf="@id/tv_in_battery_vol_title"
                app:layout_constraintRight_toRightOf="@id/tv_in_battery_vol_title"
                app:layout_constraintBottom_toTopOf="@id/tv_in_battery_vol_title"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/tv_in_battery_cap_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_20"
                android:text="@string/Label_Remaining_Power"
                android:textColor="@color/color_2D2D2D"
                android:textSize="@dimen/sp_12"
                app:layout_constraintVertical_chainStyle="packed"
                app:layout_constraintLeft_toRightOf="@id/tv_in_battery_vol_title"
                app:layout_constraintTop_toBottomOf="@id/tv_in_battery_cap"
                app:layout_constraintBottom_toBottomOf="parent" />

            <TextView
                android:id="@+id/tv_in_battery_cap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_3"
                android:text="--"
                android:textSize="@dimen/sp_12"
                app:layout_constraintLeft_toLeftOf="@id/tv_in_battery_cap_title"
                app:layout_constraintRight_toRightOf="@id/tv_in_battery_cap_title"
                app:layout_constraintBottom_toTopOf="@id/tv_in_battery_cap_title"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/tv_in_battery"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:text="@string/string_in_battery"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_10"
            android:layout_marginBottom="@dimen/dp_3"
            app:layout_constraintLeft_toLeftOf="@id/layout_in_battery"
            app:layout_constraintBottom_toTopOf="@id/layout_in_battery" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_out_battery"
            android:layout_width="0dp"
            android:layout_height="@dimen/dp_86"
            android:background="@drawable/stroke_8e8e94_radius_2_bg"
            app:layout_constraintLeft_toRightOf="@id/iv_drtk_battery"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/iv_drtk_battery"
            android:layout_marginTop="@dimen/dp_65"
            android:layout_marginLeft="@dimen/dp_30">


            <View
                android:id="@+id/div"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"
                app:layout_constraintTop_toTopOf="parent"
                android:background="@color/color_E6E6E6"
                app:layout_constraintBottom_toBottomOf="parent"/>


            <TextView
                android:id="@+id/tv_out_battery_one_vol_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:text="@string/Label_Voltage"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/color_2D2D2D"
                app:layout_constraintTop_toBottomOf="@id/tv_out_battery_one_vol"
                app:layout_constraintBottom_toBottomOf="@id/div"
                app:layout_constraintLeft_toLeftOf="parent"/>

            <TextView
                android:id="@+id/tv_out_battery_one_cap_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_60"
                android:text="@string/Label_Remaining_Power"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/color_2D2D2D"
                app:layout_constraintTop_toBottomOf="@id/tv_out_battery_one_cap"
                app:layout_constraintBottom_toBottomOf="@id/div"
                app:layout_constraintLeft_toLeftOf="parent" />

            <TextView
                android:id="@+id/tv_out_battery_one_vol"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"
                android:textSize="@dimen/sp_12"
                app:layout_constraintLeft_toLeftOf="@id/tv_out_battery_one_vol_title"
                app:layout_constraintRight_toRightOf="@id/tv_out_battery_one_vol_title"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@id/tv_out_battery_one_vol_title"/>


            <TextView
                android:id="@+id/tv_out_battery_one_cap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"
                android:textSize="@dimen/sp_12"
                app:layout_constraintLeft_toLeftOf="@id/tv_out_battery_one_cap_title"
                app:layout_constraintRight_toRightOf="@id/tv_out_battery_one_cap_title"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@id/tv_out_battery_one_cap_title"/>


            <TextView
                android:id="@+id/tv_out_battery_two_vol"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"
                android:textSize="@dimen/sp_12"
                app:layout_constraintTop_toBottomOf="@id/div"
                app:layout_constraintLeft_toLeftOf="@id/tv_out_battery_two_vol_title"
                app:layout_constraintRight_toRightOf="@id/tv_out_battery_two_vol_title"
                app:layout_constraintBottom_toTopOf="@id/tv_out_battery_two_vol_title" />


            <TextView
                android:id="@+id/tv_out_battery_two_vol_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Label_Voltage"
                android:textSize="@dimen/sp_12"
                android:layout_marginLeft="@dimen/dp_10"
                android:textColor="@color/color_2D2D2D"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_out_battery_two_vol"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <TextView
                android:id="@+id/tv_out_battery_two_cap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"
                android:textSize="@dimen/sp_12"
                app:layout_constraintTop_toBottomOf="@id/div"
                app:layout_constraintBottom_toTopOf="@id/tv_out_battery_two_cap_title"
                app:layout_constraintLeft_toLeftOf="@id/tv_out_battery_two_cap_title"
                app:layout_constraintRight_toRightOf="@id/tv_out_battery_two_cap_title"/>



            <TextView
                android:id="@+id/tv_out_battery_two_cap_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_60"
                android:text="@string/Label_Remaining_Power"
                android:textColor="@color/color_2D2D2D"
                android:textSize="@dimen/sp_12"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_out_battery_two_cap"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>



        <TextView
            android:id="@+id/tv_out_battery"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_30"
            android:gravity="center_vertical"
            android:text="@string/string_out_battery"
            android:textColor="@color/black"
            android:layout_marginBottom="@dimen/dp_3"
            android:textSize="@dimen/sp_10"
            app:layout_constraintLeft_toLeftOf="@id/layout_out_battery"
            app:layout_constraintBottom_toTopOf="@id/layout_out_battery" />


        <View
            android:id="@+id/div_battery_warn"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:layout_marginLeft="@dimen/dp_10"
            app:layout_constraintTop_toBottomOf="@id/iv_drtk_battery"
            android:background="@color/color_D8D8D8"
            android:layout_marginTop="@dimen/dp_24"/>


        <TextView
            android:id="@+id/tv_battery_warn_in"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_5"
            android:text="@string/in_low_battery_alarm"
            android:textSize="@dimen/sp_12"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/div_battery_warn" />


        <androidx.appcompat.widget.AppCompatSeekBar
            android:id="@+id/sb_battery_earn_in"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_20"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:focusable="true"
            android:max="50"
            android:maxHeight="@dimen/dp_2"
            android:min="0"
            android:minHeight="@dimen/dp_3"
            android:progress="20"
            android:progressDrawable="@drawable/icon_planset_seek_bar_progress"
            android:thumb="@drawable/icon_planset_seek_bar_thumb"
            app:layout_constraintTop_toBottomOf="@id/tv_battery_warn_in" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            app:layout_constraintTop_toBottomOf="@id/sb_battery_earn_in"
            android:layout_marginLeft="@dimen/dp_10"/>

        <TextView
            android:id="@+id/tv_battery_warn_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_10"
            android:text="@string/out_low_battery_alarm"
            android:textSize="@dimen/sp_12"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/sb_battery_earn_in" />


        <androidx.appcompat.widget.AppCompatSeekBar
            android:id="@+id/sb_battery_warn_out"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_20"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:focusable="true"
            android:max="50"
            android:maxHeight="@dimen/dp_2"
            android:min="0"
            android:minHeight="@dimen/dp_3"
            android:progress="20"
            android:progressDrawable="@drawable/icon_planset_seek_bar_progress"
            android:thumb="@drawable/icon_planset_seek_bar_thumb"
            app:layout_constraintTop_toBottomOf="@id/tv_battery_warn_out" />

        <View
            android:id="@+id/div_battery"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            app:layout_constraintTop_toBottomOf="@id/sb_battery_warn_out" />


        <!--设备状态-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_devices_state"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="@dimen/dp_8"
            app:layout_constraintTop_toBottomOf="@id/div_battery">

            <TextView
                android:id="@+id/tv_devices_state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string_devices_state"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_12"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_sdcard_state"
                android:layout_width="@dimen/dp_51"
                android:layout_height="@dimen/dp_45"
                android:layout_marginLeft="@dimen/dp_10"
                app:layout_constraintTop_toBottomOf="@id/tv_devices_state"
                app:layout_constraintLeft_toLeftOf="@id/tv_devices_state"
                android:layout_marginTop="@dimen/dp_11"
                android:src="@drawable/drtk_sdcard_state"/>


            <View
                android:id="@+id/view_state_bg"
                android:layout_width="0dp"
                android:layout_height="@dimen/dp_40"
                app:layout_constraintTop_toTopOf="@id/iv_sdcard_state"
                app:layout_constraintBottom_toBottomOf="@id/iv_sdcard_state"
                app:layout_constraintLeft_toRightOf="@id/iv_sdcard_state"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginLeft="@dimen/dp_15"
                android:background="@drawable/stroke_8e8e94_radius_2_bg"/>


            <TextView
                android:id="@+id/tv_sd_state_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/tv_sd_state"
                app:layout_constraintBottom_toBottomOf="@id/view_state_bg"
                app:layout_constraintLeft_toLeftOf="@id/view_state_bg"
                android:layout_marginLeft="@dimen/dp_16"
                android:text="@string/flight_plane_SD"
                android:textSize="@dimen/sp_11" />

            <TextView
                android:id="@+id/tv_sd_state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"
                android:textSize="@dimen/sp_11"
                android:textColor="@color/black"
                app:layout_constraintVertical_chainStyle="packed"
                app:layout_constraintLeft_toLeftOf="@id/tv_sd_state_title"
                app:layout_constraintRight_toRightOf="@id/tv_sd_state_title"
                app:layout_constraintBottom_toTopOf="@id/tv_sd_state_title"
                app:layout_constraintTop_toTopOf="@id/view_state_bg"/>


            <TextView
                android:id="@+id/tv_type_c_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_60"
                android:text="typeC"
                android:textSize="@dimen/sp_11"
                app:layout_constraintLeft_toLeftOf="@id/view_state_bg"
                app:layout_constraintTop_toBottomOf="@id/tv_type_c_state"
                app:layout_constraintBottom_toBottomOf="@id/view_state_bg"/>

            <TextView
                android:id="@+id/tv_type_c_state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"
                android:textSize="@dimen/sp_11"
                android:textColor="@color/black"
                app:layout_constraintVertical_chainStyle="packed"
                app:layout_constraintLeft_toLeftOf="@id/tv_type_c_title"
                app:layout_constraintRight_toRightOf="@id/tv_type_c_title"
                app:layout_constraintBottom_toTopOf="@id/tv_type_c_title"
                app:layout_constraintTop_toTopOf="@id/view_state_bg" />


            <View
                android:id="@+id/div_devices_state"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"
                android:background="@color/color_D8D8D8"
                android:layout_marginTop="@dimen/dp_10"
                app:layout_constraintTop_toBottomOf="@id/iv_sdcard_state"/>


        </androidx.constraintlayout.widget.ConstraintLayout>


        <!--照明-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_light"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_35"
            app:layout_constraintTop_toBottomOf="@id/layout_devices_state">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string_drtk_light"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_12"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_drtk_light"
                android:layout_width="@dimen/dp_35"
                android:layout_height="@dimen/dp_24"
                android:src="@drawable/selector_hand_control"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@color/color_D8D8D8"/>



        </androidx.constraintlayout.widget.ConstraintLayout>


        <!--找基站-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_find_station"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_35"
            app:layout_constraintTop_toBottomOf="@id/layout_light">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string_find_station"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_12"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/tv_find_station"
                android:layout_width="@dimen/dp_47"
                android:layout_height="wrap_content"
                android:text="@string/string_find_station"
                android:gravity="center"
                android:paddingTop="@dimen/dp_2"
                android:paddingBottom="@dimen/dp_2"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_11"
                android:background="@drawable/bg_ff5800_r3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@color/color_D8D8D8"/>

        </androidx.constraintlayout.widget.ConstraintLayout>


        <!--传感器设置-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_sensor"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_35"
            app:layout_constraintTop_toBottomOf="@id/layout_find_station">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string_sensor_settings"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_12"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@color/color_D8D8D8"/>


        </androidx.constraintlayout.widget.ConstraintLayout>

        <!--高级设置-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_senior_setting"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_35"
            app:layout_constraintTop_toBottomOf="@id/layout_sensor">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/advanced_settings"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_12"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@color/color_D8D8D8"/>


        </androidx.constraintlayout.widget.ConstraintLayout>

        <!--远程关机-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_turn_off"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/layout_senior_setting">

            <TextView
                android:id="@+id/tv_close_title"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_35"
                android:text="@string/string_remote_shutdown"
                android:gravity="center"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_12"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_close_drtk"
                android:layout_width="@dimen/dp_40"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_ff5800_r3"
                android:gravity="center"
                android:paddingTop="@dimen/dp_2"
                android:paddingBottom="@dimen/dp_2"
                android:text="@string/string_shutdown"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_11"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/string_close_tip"
                app:layout_constraintTop_toBottomOf="@id/tv_close_title"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginRight="@dimen/dp_50"
                android:textSize="@dimen/sp_10"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="@dimen/dp_20" />

        </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>